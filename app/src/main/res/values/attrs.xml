<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LoadingLayout">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="retryView" format="reference" />
        <attr name="emptyView" format="reference" />
    </declare-styleable>

    <!-- loadingView的自定义属性-->
    <declare-styleable name="UGLoadingView">
        <attr name="borderWidth" format="dimension" />
        <attr name="maxAngle" format="integer" />
        <attr name="borderColor" format="color" />
        <attr name="duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="AdvanceImageView">
        <attr name="AI_isCircle" format="boolean" />
        <attr name="AI_cornerRadiusPercent" format="float" />
        <attr name="AI_cornerRadius" format="dimension" />
        <attr name="AI_corners" format="integer" />
        <attr name="AI_bgStroke" format="dimension" />
        <attr name="AI_bgDrawable" format="reference" />
        <attr name="AI_fgDrawable" format="reference" />
    </declare-styleable>
</resources>